P=_1sp__1SBKTTLC_ChangeTotal    |              |@Date D,@Kind C(1),@MonthOfQuart I,@FlAdd I,@Sum N(15,3),@AccDtId C(9),@AccKtId C(9),@CurId C(9),@DtFlags Y,@KtFlags Y|{\
SET NOCOUNT ON
DECLARE @Ob1 NUMERIC(15,3),@Ob2 NUMERIC(15,3),@Ob3 NUMERIC(15,3)
SELECT @Ob1=0,@Ob2=0,@Ob3=0
IF      @MonthOfQuart=0 IF @FlAdd=1 SELECT @Ob1=@Sum ELSE SELECT @Ob1=-@Sum
ELSE IF @MonthOfQuart=1 IF @FlAdd=1 SELECT @Ob2=@Sum ELSE SELECT @Ob2=-@Sum
ELSE IF @MonthOfQuart=2 IF @FlAdd=1 SELECT @Ob3=@Sum ELSE SELECT @Ob3=-@Sum
Update _1SBKTTLC set OB1=OB1+@Ob1,OB2=OB2+@Ob2,OB3=OB3+@Ob3,DTFLAGS=@DtFlags,KTFLAGS=@KtFlags
where  DATE=@Date and KIND=@Kind and ACCKTID=@AccKtId and ACCDTID=@AccDtId and CURRID=@CurId 
IF @@ROWCOUNT=0 BEGIN
    INSERT INTO _1SBKTTLC VALUES
    (
        @Date,@AccDtId,@AccKtId,@CurId,@Kind,@Ob1,@Ob2,@Ob3,@DtFlags,@KtFlags
    )
END
\}
