P=_1sp__1SENTRY_AddSbSels       |              |@DateTimeDocId C(23),@Number I,@CorNo I,@AccDtId C(9),@AccKtId C(9),@DTSC0 C(23),@VDTSC0 I,@KTSC0 C(23),@VKTSC0 I,@DTSC1 C(23),@VDTSC1 I,@KTSC1 C(23),@VKTSC1 I,@DTSC2 C(23),@VDTSC2 I,@KTSC2 C(23),@VKTSC2 I|{\
SET NOCOUNT ON
DECLARE @HasIndex INTEGER,@DocId CHAR(9)
SELECT @DocId=SUBSTRING(@DateTimeDocId,15,9)

IF @AccDtId<>'     0   ' BEGIN
    EXECUTE _1sp_SbKind_HasIndex @VDTSC0, @HasIndex OUTPUT
    IF @VDTSC0<>0 AND @HasIndex=1
        INSERT INTO _1SSBSEL ( SBKINDID,VAL,DOCID,DATE_TIME_DOCID,NUMBER,CORNO )
        VALUES( @VDTSC0,@DTSC0,@DocId,@DateTimeDocId,@Number,@CorNo )
    EXECUTE _1sp_SbKind_HasIndex @VDTSC1, @HasIndex OUTPUT
    IF @VDTSC1<>0 AND @HasIndex=1
        INSERT INTO _1SSBSEL ( SBKINDID,VAL,DOCID,DATE_TIME_DOCID,NUMBER,CORNO )
        VALUES( @VDTSC1,@DTSC1,@DocId,@DateTimeDocId,@Number,@CorNo )
    EXECUTE _1sp_SbKind_HasIndex @VDTSC2, @HasIndex OUTPUT
    IF @VDTSC2<>0 AND @HasIndex=1
        INSERT INTO _1SSBSEL ( SBKINDID,VAL,DOCID,DATE_TIME_DOCID,NUMBER,CORNO )
        VALUES( @VDTSC2,@DTSC2,@DocId,@DateTimeDocId,@Number,@CorNo )
END
IF @AccKtId<>'     0   ' BEGIN
    EXECUTE _1sp_SbKind_HasIndex @VKTSC0, @HasIndex OUTPUT
    IF @VKTSC0<>0 AND @HasIndex=1
      AND (@VKTSC0 <> @VDTSC0 OR @KTSC0 <> @DTSC0) AND (@VKTSC0 <> @VDTSC1 OR @KTSC0 <> @DTSC1) AND (@VKTSC0 <> @VDTSC2 OR @KTSC0 <> @DTSC2) 
        INSERT INTO _1SSBSEL ( SBKINDID,VAL,DOCID,DATE_TIME_DOCID,NUMBER,CORNO )
        VALUES( @VKTSC0,@KTSC0,@DocId,@DateTimeDocId,@Number,@CorNo )
    EXECUTE _1sp_SbKind_HasIndex @VKTSC1, @HasIndex OUTPUT
    IF @VKTSC1<>0 AND @HasIndex=1
      AND (@VKTSC1 <> @VDTSC0 OR @KTSC1 <> @DTSC0) AND (@VKTSC1 <> @VDTSC1 OR @KTSC1 <> @DTSC1) AND (@VKTSC1 <> @VDTSC2 OR @KTSC1 <> @DTSC2) 
        INSERT INTO _1SSBSEL ( SBKINDID,VAL,DOCID,DATE_TIME_DOCID,NUMBER,CORNO )
        VALUES( @VKTSC1,@KTSC1,@DocId,@DateTimeDocId,@Number,@CorNo )
    EXECUTE _1sp_SbKind_HasIndex @VKTSC2, @HasIndex OUTPUT
    IF @VKTSC2<>0 AND @HasIndex=1
      AND (@VKTSC2 <> @VDTSC0 OR @KTSC2 <> @DTSC0) AND (@VKTSC2 <> @VDTSC1 OR @KTSC2 <> @DTSC1) AND (@VKTSC2 <> @VDTSC2 OR @KTSC2 <> @DTSC2) 
        INSERT INTO _1SSBSEL ( SBKINDID,VAL,DOCID,DATE_TIME_DOCID,NUMBER,CORNO )
        VALUES( @VKTSC2,@KTSC2,@DocId,@DateTimeDocId,@Number,@CorNo )
END
\}
