#==PROCEDURE 
# Name                          |Descr         |Parameters                              |Source                                                                          
P=_1sp__1SBKTTLC_ChangeTotals   |              |@FlAdd I,@AccDate D,@Date D,@Sum N(15,3),@CurSum N(15,3),@Amount N(15,3),@AccDtId C(9),@AccKtId C(9),@CurId C(9),@DtFlags Y,@KtFlags Y|{\
SET NOCOUNT ON
DECLARE @CurDate DATETIME, @MonthOfQuart INTEGER
EXECUTE _1sp_GetBeginOfPeriod @Date, 'Q', @CurDate OUTPUT
SELECT @MonthOfQuart=DATEPART(mm,@Date)-DATEPART(mm,@CurDate)
IF @CurDate<=@AccDate BEGIN
    IF @Sum<>0
        EXECUTE _1sp__1SBKTTLC_ChangeTotal @CurDate,'1',@MonthOfQuart,@FlAdd,@Sum,@AccDtId,@AccKtId,@CurId,@DtFlags,@KtFlags
    IF @CurSum<>0
        EXECUTE _1sp__1SBKTTLC_ChangeTotal @CurDate,'2',@MonthOfQuart,@FlAdd,@CurSum,@AccDtId,@AccKtId,@CurId,@DtFlags,@KtFlags
    IF @Amount<>0
        EXECUTE _1sp__1SBKTTLC_ChangeTotal @CurDate,'3',@MonthOfQuart,@FlAdd,@Amount,@AccDtId,@AccKtId,@CurId,@DtFlags,@KtFlags

END
\}
