#==PROCEDURE 
# Name                          |Descr         |Parameters                              |Source                                                                          
P=_1sp_GetParentCode            |              |@Code C(11), @Level I, @NewCode C(11) O |{\
DECLARE @Len INTEGER, @Ind INTEGER
SELECT @Len=0
SELECT @Len=CHARINDEX('.',@Code)
IF @Len<1
    SELECT @NewCode=@Code
ELSE BEGIN
    WHILE @Level>1 BEGIN
	     SELECT @Ind=CHARINDEX( '.', SUBSTRING( @Code, @Len+1, 255 ) )
    IF @Ind=0 BREAK
	     SELECT @Len=@Len+@Ind
	     SELECT @Level=@Level-1
    END
    SELECT @NewCode=SUBSTRING( @Code, 1, @Len )
END
\}
