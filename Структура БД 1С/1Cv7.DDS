#==PROCEDURE 
# Name                          |Descr         |Parameters                              |Source                                                                          
P=_1sp__1SBKTTL_ChangeTotals    |              |@ProvPart I,@FlAdd I,@AccDate D,@Date D,@Sum N(15,3),@CurSum N(15,3),@Amount N(15,3),@AccId C(9),@Currency C(9),@Flags Y,@SC0 C(13),@VSC0 I,@OSC0 Y,@SC1 C(13),@VSC1 I,@OSC1 Y,@SC2 C(13),@VSC2 I,@OSC2 Y|{\
SET NOCOUNT ON
DECLARE @UpdateOb INTEGER, @CurDate DATETIME, @MonthOfQuart INTEGER, @CurId CHAR(9)
,@_SC0 CHAR(13),@_SC1 CHAR(13),@_SC2 CHAR(13)
SELECT  @UpdateOb=1
EXECUTE _1sp_GetBeginOfPeriod @Date, 'Q', @CurDate OUTPUT
SELECT @MonthOfQuart=DATEPART(mm,@Date)-DATEPART(mm,@CurDate)
IF (@Flags&1)>0
    SELECT @CurId=@Currency
ELSE
    SELECT @CurId='     0   '
IF @OSC0=1
    SELECT @_SC0='             '
ELSE
    SELECT @_SC0=@SC0
IF @OSC1=1
    SELECT @_SC1='             '
ELSE
    SELECT @_SC1=@SC1
IF @OSC2=1
    SELECT @_SC2='             '
ELSE
    SELECT @_SC2=@SC2
WHILE @CurDate<=@AccDate BEGIN
    IF @Sum<>0 BEGIN
        EXECUTE _1sp__1SBKTTL_ChangeTotal @CurDate,'1',@MonthOfQuart,@ProvPart,@FlAdd,@Sum,@UpdateOb,@AccId,@CurId,@Flags,@_SC0,@VSC0,@OSC0,@_SC1,@VSC1,@OSC1,@_SC2,@VSC2,@OSC2
        EXECUTE _1sp__1SBKTTL_ChangeTotal @CurDate,'4',@MonthOfQuart,@ProvPart,@FlAdd,@Sum,@UpdateOb,@AccId,@CurId,@Flags,@_SC0,@VSC0,@OSC0,@_SC1,@VSC1,@OSC1,@_SC2,@VSC2,@OSC2
        IF @OSC0=1 OR @OSC1=1 OR @OSC2=1
            EXECUTE _1sp__1SBKTTL_ChangeTotal @CurDate,'7',@MonthOfQuart,@ProvPart,@FlAdd,@Sum,@UpdateOb,@AccId,@CurId,@Flags,@SC0,@VSC0,@OSC0,@SC1,@VSC1,@OSC1,@SC2,@VSC2,@OSC2
    END
    IF @CurSum<>0 AND (@Flags&1)>0 BEGIN
        EXECUTE _1sp__1SBKTTL_ChangeTotal @CurDate,'2',@MonthOfQuart,@ProvPart,@FlAdd,@CurSum,@UpdateOb,@AccId,@CurId,@Flags,@_SC0,@VSC0,@OSC0,@_SC1,@VSC1,@OSC1,@_SC2,@VSC2,@OSC2
        EXECUTE _1sp__1SBKTTL_ChangeTotal @CurDate,'5',@MonthOfQuart,@ProvPart,@FlAdd,@CurSum,@UpdateOb,@AccId,@CurId,@Flags,@_SC0,@VSC0,@OSC0,@_SC1,@VSC1,@OSC1,@_SC2,@VSC2,@OSC2
        IF @OSC0=1 OR @OSC1=1 OR @OSC2=1
            EXECUTE _1sp__1SBKTTL_ChangeTotal @CurDate,'8',@MonthOfQuart,@ProvPart,@FlAdd,@CurSum,@UpdateOb,@AccId,@CurId,@Flags,@SC0,@VSC0,@OSC0,@SC1,@VSC1,@OSC1,@SC2,@VSC2,@OSC2
    END
    IF @Amount<>0 AND (@Flags&2)>0 BEGIN
        EXECUTE _1sp__1SBKTTL_ChangeTotal @CurDate,'3',@MonthOfQuart,@ProvPart,@FlAdd,@Amount,@UpdateOb,@AccId,@CurId,@Flags,@_SC0,@VSC0,@OSC0,@_SC1,@VSC1,@OSC1,@_SC2,@VSC2,@OSC2
        IF @OSC0=1 OR @OSC1=1 OR @OSC2=1
            EXECUTE _1sp__1SBKTTL_ChangeTotal @CurDate,'9',@MonthOfQuart,@ProvPart,@FlAdd,@Amount,@UpdateOb,@AccId,@CurId,@Flags,@SC0,@VSC0,@OSC0,@SC1,@VSC1,@OSC1,@SC2,@VSC2,@OSC2
    END
    SELECT @UpdateOb=0
    SELECT @CurDate=DATEADD(mm,3,@CurDate)
END
\}
#
#===============================================================================
#==PROCEDURE 
# Name                          |Descr         |Parameters                              |Source                                                                          
P=_1sp__1SENTRY_NewEntry        |              |@CurDate D,@RowId I,@Total I,@SbSel I,@AccSel I,@Add I|{\
SET NOCOUNT ON
DECLARE @DateTimeDocId CHAR(23),@Number INTEGER,@CorNo INTEGER,@AccDtId CHAR(9),@AccKtId CHAR(9)
       ,@Sum NUMERIC(14,2),@CurId CHAR(9),@CurSum NUMERIC(14,2),@Amount NUMERIC(14,3)
       ,@ProvKind CHAR(1), @DtFlags TINYINT, @KtFlags TINYINT,@Active CHAR(1)
       ,@DTSC0 CHAR(23),@VDTSC0 INTEGER,@ODTSC0 INTEGER,@KTSC0 CHAR(23),@VKTSC0 INTEGER,@OKTSC0 INTEGER
       ,@DTSC1 CHAR(23),@VDTSC1 INTEGER,@ODTSC1 INTEGER,@KTSC1 CHAR(23),@VKTSC1 INTEGER,@OKTSC1 INTEGER
       ,@DTSC2 CHAR(23),@VDTSC2 INTEGER,@ODTSC2 INTEGER,@KTSC2 CHAR(23),@VKTSC2 INTEGER,@OKTSC2 INTEGER
DECLARE @Date DATETIME
SELECT @RowId=ROW_ID,@Number=NUMBER,@CorNo=CORNO,@DateTimeDocId=DATE_TIME_DOCID,@AccDtId=ACCDTID,@AccKtId=ACCKTID,@Active=ACTIVE
      ,@Sum=SUM_,@CurId=CURRID,@CurSum=CURSUM,@Amount=AMOUNT,@ProvKind=PROVKIND,@DtFlags=DTFLAGS,@KtFlags=KTFLAGS
       ,@DTSC0=DTSC0,@VDTSC0=VDTSC0,@ODTSC0=ODTSC0,@KTSC0=KTSC0,@VKTSC0=VKTSC0,@OKTSC0=OKTSC0
       ,@DTSC1=DTSC1,@VDTSC1=VDTSC1,@ODTSC1=ODTSC1,@KTSC1=KTSC1,@VKTSC1=VKTSC1,@OKTSC1=OKTSC1
       ,@DTSC2=DTSC2,@VDTSC2=VDTSC2,@ODTSC2=ODTSC2,@KTSC2=KTSC2,@VKTSC2=VKTSC2,@OKTSC2=OKTSC2
FROM _1SENTRY WHERE ROW_ID=@RowId
SELECT @Date=CONVERT(CHAR(8),@DateTimeDocId)
IF @@ROWCOUNT>0 BEGIN
    IF @Total=1 AND @Active=' ' BEGIN
        IF( (@ProvKind=' ') OR (@CorNo>0)  )
            EXECUTE _1sp__1SBKTTLC_ChangeTotals @Add, @CurDate,@Date,@Sum,@CurSum,@Amount,@AccDtId,@AccKtId,@CurId,@DtFlags,@KtFlags
        IF( ( (@ProvKind=' ') OR (@ProvKind='D') ) AND @AccDtId <> '     0   ')
            EXECUTE _1sp__1SBKTTL_ChangeTotals 0,@Add,@CurDate,@Date,@Sum,@CurSum,@Amount,@AccDtId,@CurId,@DtFlags
,@DTSC0,@VDTSC0,@ODTSC0,@DTSC1,@VDTSC1,@ODTSC1,@DTSC2,@VDTSC2,@ODTSC2
        IF( ((@ProvKind=' ') OR (@ProvKind='K')) AND @AccKtId <> '     0   ')
            EXECUTE _1sp__1SBKTTL_ChangeTotals 1,@Add,@CurDate,@Date,@Sum,@CurSum,@Amount,@AccKtId,@CurId,@KtFlags
,@KTSC0,@VKTSC0,@OKTSC0,@KTSC1,@VKTSC1,@OKTSC1,@KTSC2,@VKTSC2,@OKTSC2
    END
    IF @SbSel=1 BEGIN
        EXECUTE _1sp__1SENTRY_AddSbSels @DateTimeDocId,@Number,@CorNo,@AccDtId,@AccKtId,@DTSC0,@VDTSC0,@KTSC0,@VKTSC0,@DTSC1,@VDTSC1,@KTSC1,@VKTSC1,@DTSC2,@VDTSC2,@KTSC2,@VKTSC2
    END
    IF @AccSel=1 BEGIN
        EXECUTE _1sp__1SENTRY_AddAccSels @DateTimeDocId,@Number,@CorNo,@AccDtId,@AccKtId
    END
END
\}
