#==PROCEDURE 
# Name                          |Descr         |Parameters                              |Source                                                                          
P=_1sp__1SENTRY_AddAccSels      |              |@DateTimeDocId C(23),@Number I,@CorNo I,@AccDtId C(9),@AccKtId C(9)|{\
SET NOCOUNT ON
IF @AccDtId='     0   ' AND @AccKtId='     0   ' RETURN
DECLARE @DtCode CHAR(11),@DtLevel INTEGER,@KtCode CHAR(11),@KtLevel INTEGER
DECLARE @TmpLevel INTEGER,@PlanId INTEGER,@DocId CHAR(9)
SELECT @DocId=SUBSTRING(@DateTimeDocId,15,9)
SELECT @DtCode=SCHKOD,@DtLevel=LEVEL_,@PlanId=PLANID FROM _1SACCS WHERE ID=@AccDtId
SELECT @KtCode=SCHKOD,@KtLevel=LEVEL_,@PlanId=PLANID FROM _1SACCS WHERE ID=@AccKtId
SELECT @DtLevel=ISNULL(@DtLevel, 0), @KtLevel=ISNULL(@KtLevel, 0)
WHILE @DtLevel>@KtLevel BEGIN
    IF @DtLevel<=0 OR 0=0 BEGIN
        INSERT _1SACCSEL ( ACCID,DOCID,DATE_TIME_DOCID,NUMBER,CORNO,DT,KT ) VALUES( @AccDtId,@DocId,@DateTimeDocId,@Number,@CorNo,'*',' ' )
        SELECT @DtLevel=@DtLevel-1
    END
    ELSE BEGIN
        SELECT @DtLevel=0
    END
    IF @DtLevel=0 BREAK
    EXECUTE _1sp_GetParentCode @DtCode, @DtLevel, @DtCode OUTPUT
    SELECT @AccDtId=ID FROM _1SACCS(NOLOCK) WHERE SCHKOD=@DtCode AND PLANID=@PlanId
    IF @@ROWCOUNT=0 BREAK
END
WHILE @KtLevel>@DtLevel BEGIN
    IF @KtLevel<=0 OR 0=0 BEGIN
        INSERT _1SACCSEL ( ACCID,DOCID,DATE_TIME_DOCID,NUMBER,CORNO,DT,KT ) VALUES( @AccKtId,@DocId,@DateTimeDocId,@Number,@CorNo,' ','*' )
        SELECT @KtLevel=@KtLevel-1
    END
    ELSE BEGIN
        SELECT @KtLevel=0
    END
    IF @KtLevel=0 BREAK
    EXECUTE _1sp_GetParentCode @KtCode, @KtLevel, @KtCode OUTPUT
    SELECT @AccKtId=ID FROM _1SACCS(NOLOCK) WHERE SCHKOD=@KtCode AND PLANID=@PlanId
    IF @@ROWCOUNT=0 BREAK
END
IF @DtLevel>0 AND 0<>0 BEGIN
    SELECT @DtLevel=0
    EXECUTE _1sp_GetParentCode @DtCode, @DtLevel, @DtCode OUTPUT
    SELECT @AccDtId=ID FROM _1SACCS(NOLOCK) WHERE SCHKOD=@DtCode AND PLANID=@PlanId
    EXECUTE _1sp_GetParentCode @KtCode, @DtLevel, @KtCode OUTPUT /*DtLevel=KtLevel*/
    SELECT @AccKtId=ID FROM _1SACCS(NOLOCK) WHERE SCHKOD=@KtCode AND PLANID=@PlanId
END
WHILE @DtLevel>0 BEGIN
    IF @DtCode=@KtCode BEGIN
        INSERT _1SACCSEL ( ACCID,DOCID,DATE_TIME_DOCID,NUMBER,CORNO,DT,KT ) VALUES( @AccDtId,@DocId,@DateTimeDocId,@Number,@CorNo,'*','*' )
        SELECT @DtLevel=@DtLevel-1
        IF @DtLevel=0 BREAK
        EXECUTE _1sp_GetParentCode @DtCode, @DtLevel, @DtCode OUTPUT
        SELECT @AccDtId=ID FROM _1SACCS(NOLOCK) WHERE SCHKOD=@DtCode AND PLANID=@PlanId
        SELECT @KtCode=@DtCode
    END
    ELSE BEGIN
        INSERT _1SACCSEL ( ACCID,DOCID,DATE_TIME_DOCID,NUMBER,CORNO,DT,KT ) VALUES( @AccDtId,@DocId,@DateTimeDocId,@Number,@CorNo,'*',' ' )
        INSERT _1SACCSEL ( ACCID,DOCID,DATE_TIME_DOCID,NUMBER,CORNO,DT,KT ) VALUES( @AccKtId,@DocId,@DateTimeDocId,@Number,@CorNo,' ','*' )
        SELECT @DtLevel=@DtLevel-1
        IF @DtLevel=0 BREAK
        EXECUTE _1sp_GetParentCode @DtCode, @DtLevel, @DtCode OUTPUT
        SELECT @AccDtId=ID FROM _1SACCS(NOLOCK) WHERE SCHKOD=@DtCode AND PLANID=@PlanId
        EXECUTE _1sp_GetParentCode @KtCode, @DtLevel, @KtCode OUTPUT
 /*DtLevel=KtLevel*/
        SELECT @AccKtId=ID FROM _1SACCS(NOLOCK) WHERE SCHKOD=@KtCode AND PLANID=@PlanId
    END
END
\}
